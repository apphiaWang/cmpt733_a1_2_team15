{
    "version": 1,
    "request": {
        "url": "https://api.themoviedb.org/3/movie/now_playing",
        "method": "GET",
        "params": {
            "language": false,
            "page": false,
            "region": false
        },
        "authorization": {
            "type": "QueryParam",
            "keyParam": "api_key"
        },
        "pagination": {
            "type": "offset",
            "offsetKey": "offset",
            "limitKey": "count",
            "maxCount": 50
        }
    },
    "response": {
        "ctype": "application/json",
        "tablePath": "$.results[*]",
        "schema": {
            "title": {
                "target": "$.title",
                "type": "string"
            },
            "release_date": {
                "target": "$.release_date",
                "type": "string"
            },
            "genre_ids": {
                "target": "$.genre_ids",
                "type": "list"
            },
            "overview": {
                "target": "$.overview",
                "type": "string"
            },
            "original_language": {
                "target": "$.original_language",
                "type": "string"
            },
            "popularity": {
                "target": "$.popularity",
                "type": "int"
            },
            "vote_average": {
                "target": "$.vote_average",
                "type": "float"
            },
            "vote_count": {
                "target": "$.vote_count",
                "type": "int"
            },
            "is_adult": {
                "target": "$.adult",
                "type": "boolean"
            }
        },
        "orient": "records"
    }
}