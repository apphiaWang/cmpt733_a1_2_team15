{
    "version": 1,
    "request": {
        "url": "https://api.themoviedb.org/3/movie/now_playing",
        "method": "GET",
        "params": {
            "language": false,
            "page": false,
            "region": false
        },
        "authorization": {
            "type": "QueryParam",
            "keyParam": "api_key"
        }
    },
    "response": {
        "ctype": "application/json",
        "tablePath": "$.results[*]",
        "schema": {
            "release_date": {
                "target": "$.release_date",
                "type": "string"
            },
            "title": {
                "target": "$.title",
                "type": "string"
            },
            "vote_count": {
                "target": "$.vote_count",
                "type": "int"
            },
            "vote_average": {
                "target": "$.vote_average",
                "type": "float"
            }
        },
        "orient": "records"
    }
}