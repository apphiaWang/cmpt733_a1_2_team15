{
    "version": 1,
    "request": {
        "url": "https://api.themoviedb.org/3/movie/now_playing",
        "method": "GET",
        "params": {
            "api_key": true,
            "language": false,
            "page": false,
            "region": false
        }
    },
    "examples": {
        "api_key": "'af093fe5ad9cc1c2125e2464c51b4edd'",
        "region": "'CA'"
    },
    "response": {
        "ctype": "application/json",
        "tablePath": "$.results[*]",
        "schema": {
            "release_date": {
                "target": "$.release_date",
                "type": "string"
            },
            "title": {
                "target": "$.title",
                "type": "string"
            },
            "vote_count": {
                "target": "$.vote_count",
                "type": "int"
            },
            "vote_average": {
                "target": "$.vote_average",
                "type": "float"
            }
        }
    }
}