{
    "version": 1,
    "request": {
        "url": "https://api.themoviedb.org/3/person/{person_id}/movie_credits",
        "method": "GET",
        "params": {
            "person_id": true
        },
        "authorization": {
            "type": "QueryParam",
            "keyParam": "api_key"
        }
    },
    "response": {
        "ctype": "application/json",
        "tablePath": "$.cast[*]",
        "schema": {
            "movie_id": {
                "target": "$.id",
                "type": "int"
            },
            "title": {
                "target": "$.title",
                "type": "string"
            },
            "overview": {
                "target": "$.overview",
                "type": "string"
            },
            "popularity": {
                "target": "$.popularity",
                "type": "float"
            },
            "release_date": {
                "target": "$.release_date",
                "type": "string"
            }
        },
        "orient": "records"
    }
}