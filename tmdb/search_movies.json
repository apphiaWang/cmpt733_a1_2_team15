{
    "version": 1,
    "request": {
        "url": "https://api.themoviedb.org/3/search/movie",
        "method": "GET",
        "params": {
            "query": true
        },
        "authorization": {
            "type": "QueryParam",
            "keyParam": "api_key"
        }
    },
    "response": {
        "ctype": "application/json",
        "tablePath": "$.results[*]",
        "schema": {
            "original_title": {
                "target": "$.original_title",
                "type": "string"
            },
            "original_language": {
                "target": "$.original_language",
                "type": "string"
            },
            "vote_count": {
                "target": "$.vote_count",
                "type": "int"
            },
            "vote_average": {
                "target": "$.vote_average",
                "type": "float"
            }
        },
        "orient": "records"
    }
}